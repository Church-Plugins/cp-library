{
  "name": "cp-library",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:blocks": "wp-scripts start --webpack-src-dir=blocks/ --output-path=build/blocks --config node_modules/@wordpress/scripts/config/webpack.config.js",
    "build:blocks": "wp-scripts build --webpack-src-dir=blocks/ --output-path=build/blocks --config node_modules/@wordpress/scripts/config/webpack.config.js",
    "start:src": "wp-scripts start",
    "build:src": "wp-scripts build",
    "start": "npm run start:src && npm run start:blocks",
    "build": "npm run build:src && npm run build:blocks",
    "plugin-zip": "wp-scripts plugin-zip"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.3",
    "@mui/material": "^5.0.0",
    "@wordpress/api-fetch": "^6.35.0",
    "@wordpress/compose": "^6.35.0",
    "@wordpress/data": "^9.23.0",
    "@wordpress/icons": "^9.29.0",
    "@wordpress/scripts": "^26.9.0",
    "async": "^3.2.1",
    "axios": "^1.6.7",
    "babel-loader": "8.1.0",
    "classnames": "^2.5.1",
    "env-cmd": "^10.1.0",
    "jquery": "^3.7.1",
    "js-cookie": "^3.0.5",
    "lodash.throttle": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.9",
    "react-paginate": "^8.2.0",
    "react-player": "^2.9.0",
    "react-router-dom": "^6.22.2",
    "react-scripts": "^5.0.1",
    "react-templates": "^0.5.2",
    "react-templates-loader": "^0.5.1",
    "requirejs": "^2.3.6",
    "sass": "^1.42.1",
    "screenfull": "^5.1.0",
    "styled-components": "^5.3.1",
    "tiny-relative-date": "^1.3.0",
    "web-vitals": "^1.1.2",
    "webpack-cli": "5.1.4"
  },
  "files": [
    "assets/images/**",
    "assets/js/**",
    "includes/**",
    "vendor/**",
    "dist/**",
    "build/**",
    "*.php",
    "*.md",
    "readme.txt",
    "languages/**",
    "layouts/**",
    "templates/**",
    "blocks/**",
    "block-patterns/**",
    "LICENSE",
    "*.css"
  ]
}
